{
    "name": "tarantool/symfony-lock",
    "description": "Symfony lock Tarantool store",
    "keywords": ["tarantool", "client", "pure", "nosql", "lock"],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "symfony/lock": ">=7.0",
        "tarantool/client": ">=0.9.0"
    },
    "suggest": {
        "ext-msgpack": "For using PeclPacker.",
        "rybakit/msgpack": "For using PurePacker."
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2.5",
        "rybakit/msgpack": "^0.7.0"
    },
    "autoload-dev": {
        "psr-4": {
            "": "tests"
        },
        "classmap": ["tests"]
    },
    "autoload": {
        "psr-4": {
            "Tarantool\\SymfonyLock\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Dmitry Krokhin",
            "email": "nekufa@gmail.com"
        }
    ]
}
